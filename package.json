{
  "name": "decorator-mysql",
  "version": "1.0.20",
  "description": "爱用装饰器风格查询MySQL的框架一枚鸭~",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "typings": "./dist/types/index.d.ts",
  "scripts": {
    "build": "node ./scripts/build.js",
    "test": "jest --verbose",
    "dev:docs": "vitepress dev docs",
    "build:docs": "vitepress build docs",
    "prepublishOnly": "zx ./scripts/prepublish.mjs"
  },
  "jest": {
    "rootDir": "test",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": ".*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node"
  },
  "keywords": [
    "decorator-mysql",
    "decorator",
    "mysql",
    "xuanxiaoqian"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xuanxiaoqian/decorator-mysql"
  },
  "bugs": {
    "url": "https://github.com/xuanxiaoqian/decorator-mysql/issues"
  },
  "author": "xuanxiaoqian",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.18",
    "esbuild": "^0.16.12",
    "glob": "^8.0.3",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4",
    "zx": "^7.0.8",
    "vitepress": "^1.0.0-alpha.13"
  },
  "dependencies": {
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13"
  },
  "engines": {
    "node": ">=16"
  }
}